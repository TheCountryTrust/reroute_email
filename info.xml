<?xml version="1.0"?>
<extension key="uk.countrytrust.reroute_email" type="module">
  <file>reroute_email</file>
  <name>Reroute Email</name>
  <description>Reroutes all email sent from CiviCRM to a predefined email. Used for publically available test sites</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Martin Stevens</author>
    <email>mstevens@countrytrust.org.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-05-13</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a new, undeveloped module, requires a UI or global setting in the civicrm.settings.php file</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/RerouteEmail</namespace>
    <angularModule>crmRerouteEmail</angularModule>
  </civix>
</extension>
